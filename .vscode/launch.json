{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug cub3d",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cub3D",
      "args": ["CUB/hel lo.cub"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
